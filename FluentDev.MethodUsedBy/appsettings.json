{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "FileLog": {
      "LogDir": "C:\\analyzer\\log",
      "LogName": "TraceLog_{timestamp}.log",
      "MaxFileSize": 524288000, // 500MB
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "Analyze": {
    "AssemblyIgnore": [
      "System*",
      "ActiveReports*"
    ],
    "ExportDir": "C:\\analyzer\\export",
    "MaxCalltreeDepth": 50,
    "SuppressWarning": {
      "MethodNoBody": false,
      "MethodNoBodyInterfaceOnly": true,
      "MethodNoBodyAbstractClassOnly": true
    }
  },
  "Cache": {
    "Dir": "C:\\analyzer\\cache",
    "Persist": true,
    "UseMultiCache": false
  }
}